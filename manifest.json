{
    "manifest_version": 2,
    "name": "Auto input text",
    "version": "1.0",
    "description": "Adds text to the input fields",

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js", "jquery-3.4.1.min.js"]
        }
    ],

    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "AutoInput"
    },

    "permissions":[
        "tabs",
        "storage"
    ]

}